{
  "name": "wwjs",
  "version": "2.0.0",
  "description": "wware browser runtime library",
  "browser": "src/index.js",
  "keywords": [],
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "check": "npm run lint && npm run test",
    "build": "npm run lint && webpack  --optimize-minimize --mode production --env=prod --display-modules && npm run test",
    "build-dev": "webpack --env=dev --display-modules ",
    "doc": "node_modules/.bin/jsdoc  --configure .jsdoc.json --verbose;xdg-open ./docs/wwjs/`node -p 'require(\"./package.json\").version'`/index.html",
    "v:patch": "npm version --no-git-tag-version patch",
    "start:dev": "webpack-dev-server --open --progress --colors",
    "start": "webpack-dev-server --open --progress --colors --env=prod",
    "fetch:lib": "echo fetch std lib from wware.org",
    "test": "node test/runner.node.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead",
          "modules": "amd",
          "useBuiltIns": false
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "testling": {
    "preprocess": "build/build.sh",
    "html": "spec/runner.html?src=build/output/knockout-latest.js&testling=true",
    "browsers": [
      "ie/8..latest",
      "chrome/20..latest",
      "firefox/3..latest",
      "safari/5.0.5..latest",
      "opera/11.0..latest",
      "iphone/6..latest",
      "ipad/6..latest"
    ]
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "wwjs@scm.spolo.org:/home/source/wwjs/wware.git"
  },
  "author": {
    "name": "masol",
    "email": "masol.li@gmail.com",
    "url": "https://www.wware.org"
  },
  "alternatives": {
    "precommit": "lint-staged",
    "loadjs": "^3.5.4",
    "requirejs": "^2.3.6",
    "johnnydepp": "0.0.3",
    "chai": "^4.2.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "webdriverio": "^4.14.0",
    "phantomjs-prebuilt": "^2.1.16",
    "systemjs": "^2.1.1"
  },
  "dependencies": {},
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "license": "WWARE LICENSE",
  "homepage": "https://www.wware.org",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "cash-dom": "^2.3.9",
    "compression-webpack-plugin": "^2.0.0",
    "docdash": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.3",
    "jsdoc": "^3.5.5",
    "lint-staged": "^8.0.4",
    "opn": "^5.4.0",
    "promise-polyfill": "^8.1.0",
    "string-replace-loader": "^2.1.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "whatwg-fetch": "^3.0.0"
  }
}
